<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css"> {% block stylesheets %}{% endblock %}{% block javascripts %}{% endblock %}
		</head>
		<body>

			

			<header class="topbar bg-dark text-light border-bottom">
				<form action="{{ path('search') }}" method="get" class="search-form d-flex">
					<input type="text" name="q" class="form-control form-control-sm" placeholder="Search...">
					<button class="btn btn-outline-light btn-sm ms-2">
						<i class="bi bi-search"></i>
					</button>
				</form>
			</header>

			{% include 'navigation.html.twig' %}

			<div class="main-wrapper">
				<main class="p-4"> {% block body %}{% endblock %}
					</main>
				</div>

			</body>


		</html>
